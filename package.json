{
  "name": "newar-message-sender",
  "version": "1.0.0",
  "description": "High-performance Facebook Messenger message sender with BullMQ and HTTP connection pooling",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "run-processor": "NODE_ENV=production SERVICE_TYPE=run-processor node dist/index.js",
    "message-worker": "NODE_ENV=production SERVICE_TYPE=message-worker node dist/index.js",
    "api": "NODE_ENV=production SERVICE_TYPE=api node dist/index.js",
    "migrate": "bash scripts/run-migrations.sh",
    "migrate:create-partitions": "bash scripts/create-future-partitions.sh",
    "test:performance": "tsx scripts/test-performance.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "messenger",
    "facebook",
    "bullmq",
    "queue",
    "message-sender"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^5.11.2",
    "@bull-board/express": "^5.11.2",
    "@supabase/supabase-js": "^2.39.3",
    "agentkeepalive": "^4.5.0",
    "axios": "^1.6.5",
    "bullmq": "^5.1.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
